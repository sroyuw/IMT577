          SQLTask:SqlStatementSource="-- factSalesActual&#xA;&#xA;IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factSalesActual')&#xA;BEGIN&#xA;&#x9;-- ====================================&#xA;&#x9;-- Load dimCustomer table&#xA;&#x9;-- ====================================&#xA;&#xA;&#x9;INSERT INTO dbo.factSalesActual&#xA;&#x9;(&#xA;&#x9;dimProductID&#xA;&#x9;,dimStoreID&#xA;&#x9;,dimResellerID&#xA;&#x9;,dimCustomerID&#xA;&#x9;,dimChannelID&#xA;&#x9;,dimSaleDateID&#xA;&#x9;,dimLocationID&#xA;&#x9;,factSourceSalesHeaderID&#xA;&#x9;,factSourceSalesDetailID&#xA;&#x9;,factSaleAmount&#xA;&#x9;,factSaleQuantity&#xA;&#x9;,factSaleUnitPrice&#xA;&#x9;,factSaleExtendedCost&#xA;&#x9;,factSaleTotalProfit&#xA;&#x9;)&#xA;&#x9;SELECT DISTINCT &#xA;&#x9; dbo.dimProduct.dimSourceProductID as dimProductID&#xA;&#x9;,COALESCE(dbo.dimStore.dimStoreID, -1) AS dimStoreID&#xA;&#x9;,COALESCE(dbo.dimReseller.dimResellerID, -1) AS dimResellerID&#xA;&#x9;,COALESCE(dbo.dimCustomer.dimCustomerID, -1) AS dimCustomerID&#xA;&#x9;,COALESCE(dbo.dimChannel.dimChannelID, -1) AS dimChannelID&#xA;&#x9;,COALESCE(dbo.DimDate.dimDateID,-1) AS dimSaleDateID&#xA;&#x9;,COALESCE(dbo.dimCustomer.dimLocationID, dbo.dimStore.dimLocationID, dbo.dimReseller.dimLocationID,-1) AS dimdimLocationID&#xA;&#x9;,dbo.StageSalesHeader.SalesHeaderID AS factSourceSalesHeaderID&#xA;&#x9;,dbo.StageSalesDetail.SalesDetailID AS factSourceSalesDetailID&#xA;&#x9;,dbo.StageSalesDetail.SalesAmount AS factSaleAmount&#xA;&#x9;,dbo.StageSalesDetail.SalesQuantity AS factSaleQuantity&#xA;&#x9;,CAST((SalesAmount/SalesQuantity) AS Decimal(18,2)) AS factSaleUnitPrice&#xA;&#x9;,(SalesQuantity*dimProductCost) AS factSaleExtendedCost --formula needed&#xA;&#x9;,(SalesAmount-(SalesQuantity*dimProductCost)) AS factSaleTotalProfit  --formula needed&#xA;&#xA;&#x9;FROM StageSalesHeader&#xA;&#x9;INNER JOIN StageSalesDetail&#xA;&#x9;&#x9;ON StageSalesHeader.SalesHeaderID = StageSalesDetail.SalesHeaderID&#xA;&#x9;INNER JOIN dimProduct&#xA;&#x9;&#x9;ON StageSalesDetail.ProductID = dimProduct.dimSourceProductID&#xA;&#x9;INNER JOIN dimChannel&#xA;&#x9;&#x9;ON StageSalesHeader.ChannelID = dimChannel.dimSourceChannelID&#xA;&#x9;LEFT JOIN DimDate&#xA;&#x9;&#x9;ON StageSalesHeader.Date = DimDate.FullDate&#xA;&#x9;LEFT JOIN dimCustomer&#xA;&#x9;&#x9;ON StageSalesHeader.CustomerID = dimCustomer.dimSourceCustomerID&#xA;&#x9;LEFT JOIN dimStore&#xA;&#x9;&#x9;ON StageSalesHeader.StoreID = dimStore.dimSourceStoreID&#xA;&#x9;LEFT JOIN dimReseller&#xA;&#x9;&#x9;ON StageSalesHeader.ResellerID = dimReseller.dimSourceResellerID;&#xA;&#xA;END&#xA;GO " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
