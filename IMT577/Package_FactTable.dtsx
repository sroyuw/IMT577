        <SQLTask:SqlTaskData
          SQLTask:Connection="{8ADE935E-0968-4169-9470-A1B32788E8C2}"
          SQLTask:SqlStatementSource="-- factProductSalesTarget&#xA;&#xA;IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factProductSalesTarget')&#xA;BEGIN&#xA;&#x9;-- ====================================&#xA;&#x9;-- Load factProductSalesTarget table&#xA;&#x9;-- ====================================&#xA;&#xA;&#x9;INSERT INTO dbo.factProductSalesTarget&#xA;&#x9;(&#xA;&#x9; dimProductID&#xA;&#x9;,dimTargetDateID&#xA;&#x9;,factProductTargetSalesQuantity&#xA;&#x9;)&#xA;&#x9;SELECT DISTINCT&#xA;&#x9; COALESCE(dbo.dimProduct.dimProductID,-1) AS dimProductID&#xA;&#x9;,dbo.DimDate.dimDateID AS dimTargetDateID&#xA;&#x9;,dbo.[StageProductSalesTarget].[ SalesQuantityTarget ]/365 AS factProductTargetSalesQuantity&#xA;&#x9;FROM [StageProductSalesTarget]&#xA;&#x9;LEFT JOIN dimProduct&#xA;&#x9;&#x9;ON [StageProductSalesTarget].productID = dimProduct.dimSourceproductID&#xA;&#x9;LEFT JOIN dimDate&#xA;&#x9;&#x9;ON [StageProductSalesTarget].year = dimDate.CalendarYear order by 1;&#xA;END&#xA;GO " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
